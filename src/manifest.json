{
    "name": "Leetcode Block Users",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["*://leetcode.com/problems/*"],
            "js": ["js/discussion_item.js", "js/content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "html/popup.html",
        "default_title": "Leetcode Block Users"
    }
}
